  /paybacks:
    is: [secured, workspaceId]
    description: 应还/应付款项查询(未完成)
    get:
      description: |
        目前只能列出正在还款和待放款的项目<br />
        获取所有项目的近期还款列表，当天，本月，本周<br />
        scope=all 为列出所有应还/应付款项目<br />
        scope=list 为分页列出应还/应付款项目<br />
        scope=count 为统计应还/应付款项目数量
      queryParameters:
        businessType:
          type: string
          description: LOAN | BORROW （贷款还是融资，不能为空)
        beginDate:
          type: date
          description: |
            '2015-09-30'(不传的话就是1970开始，可空）
        endDate:
          type: date
          description: |
            '2015-11-11'(不传的话时间右边开区间，可空）
        keyId:
          type: string
          description: |
            '张三0001'(合同编号搜索过滤，可空)
        states:
          type: string
          description: |
            可以传多个,如果不传就是列出所有的 见 'LoanStatusEnum'
            'FINISH(已完成)' | 'PAY_PROCESS(还款中)' | 'WAIT_LAUNCH'(待放款的)
        types:
